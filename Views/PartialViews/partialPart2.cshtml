<script>

    function getData() {
        var dbData = [];
        checkedItems = [];
        let weburl = $("#webURL").val();
        var model = { "weburl": weburl };

        $.ajax({
            type: "POST",
            url: "/Home/GetData",
            data: model,
            success: function (data) {
                console.log(data);
                $.each(data, function (index, value) {
                    $('#spanArea').append("<span class='badge badge-primary'>" + value + " - ( 150 ) </span>");
                });
            },
            error: function (error) {
                console.log(JSON.stringify(error))
            }
        })

    };

</script>